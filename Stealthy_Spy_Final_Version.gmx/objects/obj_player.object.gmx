            <kind>1</kind>
            <string>///Step Event
scr_get_input();

dist = spd/slow_factor;

if (ru_key){
    sprite_index = spr_player_back;
    image_speed =1;
    dir = 325;
    phy_position_x += dist/1.4;
    phy_position_y -= dist/1.4;
}

else if (rd_key){
    sprite_index = spr_player_front;
    image_speed =1;
    dir = 45;
    phy_position_x += dist/1.4;
    phy_position_y += dist/1.4;
}
else if (lu_key){
    sprite_index = spr_player_back;
    image_speed =1;
    dir = 225;
    phy_position_x -= dist/1.4;
    phy_position_y -= dist/1.4;
}
else if (ld_key){
    sprite_index = spr_player_front;
    image_speed =1;
    dir = 135;
    phy_position_x -= dist/1.4;
    phy_position_y += dist/1.4;
}
//Move Left
else if (left_key) {
    sprite_index = spr_player_left;
    image_speed = 1;
    dir = 180;
    phy_position_x -= dist;
}
//Move Right
else if (right_key) {
    sprite_index = spr_player_right;
    image_speed = 1;
    dir = 0;
    phy_position_x += dist;
}
//Move Up
else if (up_key) {
    sprite_index = spr_player_back ;
    image_speed = 1;
    dir = 270;
    phy_position_y -= dist;
}
//Move Down
else if (down_key) {
    sprite_index = spr_player_front;
    image_speed = 1;
    dir = 90;
    phy_position_y += dist;
}


else if (no_key){
    image_speed = 0;
    image_index = 0;
}

///Transparent Effect

if (place_meeting(x,y, obj_desk))
{
  hidden = 1;
  image_alpha=0.5;
}
else
{
  hidden = 0;
  if(cloak==0){
    image_alpha=1;
  }
}

if (place_meeting(x,y,obj_box))
{
    visible = false;
    hidden_box = 1;
}
else
{   
    visible = true;
    hidden_box = 0;
}

